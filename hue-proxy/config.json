{
    "name": "Hue Proxy",
    "version": "1",
    "slug": "hue_proxy",
    "description": "Proxy requests to Hue bridge",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "application",
    "boot": "auto",
    "options": {
      "hue_ip": "null"
    },
    "schema": {
      "hue_ip": "str"
    },
    "ports": {
      "30000/tcp": 30000
    }
  }
  